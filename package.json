{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "yarn build:ssr",
    "start": "yarn serve:ssr",
    "start:browser": "node server.js",
    "build:browser": "ng build --aot --prod",
    "serve:ssr": "node dist/server",
    "build:ssr": "yarn build:client-and-server-bundles && yarn compile:server && yarn copy:robots",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run website:server:production",
    "copy:robots": "cp src/robots.txt dist/browser/",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/cli": "~8.0.6",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/forms": "~8.0.3",
    "@angular/http": "~7.2.16",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/platform-server": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@angular/service-worker": "~8.0.3",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "angular-svg-icon": "^8.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.4",
    "express": "^4.17.3",
    "path": "^0.12.7",
    "rxjs": "~6.5.5",
    "typescript": "~3.8.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/language-service": "~8.0.3",
    "@types/node": "~13.11.0",
    "codelyzer": "~5.2.2",
    "enhanced-resolve": "^4.1.1",
    "prettier": "^2.0.2",
    "prettier-tslint": "^0.4.2",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.8.2",
    "tslib": "^1.11.1",
    "tslint": "~6.1.1",
    "webpack-cli": "^3.3.11"
  },
  "heroku-run-build-script": true
}
