{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "yarn build:ssr",
    "start": "yarn serve:ssr",
    "start:browser": "node server.js",
    "build:browser": "ng build --aot --prod",
    "serve:ssr": "node dist/server",
    "build:ssr": "yarn build:client-and-server-bundles && yarn compile:server && yarn copy:robots",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run website:server:production",
    "copy:robots": "cp src/robots.txt dist/browser/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.13",
    "@angular/cli": "~7.3.8",
    "@angular/common": "~7.2.13",
    "@angular/compiler": "~7.2.13",
    "@angular/compiler-cli": "~7.2.13",
    "@angular/core": "~7.2.13",
    "@angular/forms": "~7.2.13",
    "@angular/http": "~7.2.13",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.2.13",
    "@angular/platform-server": "~7.2.13",
    "@angular/router": "~7.2.13",
    "@angular/service-worker": "~7.2.13",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "angular-svg-icon": "^7.2.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "path": "^0.12.7",
    "prettier-tslint": "^0.4.2",
    "rxjs": "~6.4.0",
    "typescript": "~3.2.4",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/language-service": "~7.2.13",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.13.6",
    "codelyzer": "~5.0.0",
    "enhanced-resolve": "^4.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "webpack-cli": "^3.1.0"
  },
  "heroku-run-build-script": true
}
